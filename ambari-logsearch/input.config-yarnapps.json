{
  "input":[
    {
      "type":"yarn_apps_nm_stdout1",
      "rowtype":"service",
      "path":"/hadoopfs/fs*/yarn/nodemanager/log/*/*/stdout"
    },
    {
      "type":"yarn_apps_nm_stderr1",
      "rowtype":"service",
      "path":"/hadoopfs/fs*/yarn/nodemanager/log/*/*/stderr"
    },
    {
      "type":"yarn_apps_nm_stdout2",
      "rowtype":"service",
      "path":"/yarn/nodemanager/log/*/*/stdout"
    },
    {
      "type":"yarn_apps_nm_stderr2",
      "rowtype":"service",
      "path":"/yarn/nodemanager/log/*/*/stderr"
    },
    {
      "type":"yarn_apps_task1",
      "rowtype":"service",
      "path":"/hadoopfs/fs*/hadoop/yarn/log/*/*/syslog"
    },
    {
      "type":"yarn_apps_taskattempt1",
      "rowtype":"service",
      "path":"/hadoopfs/fs*/hadoop/yarn/log/*/*/syslog_attempt*"
    },
    {
      "type":"yarn_apps_task2",
      "rowtype":"service",
      "path":"/hadoop/yarn/log/*/*/syslog"
    },
    {
      "type":"yarn_apps_taskattempt2",
      "rowtype":"service",
      "path":"/hadoop/yarn/log/*/*/syslog_attempt*"
    }
   ],
  "filter":[
    {
      "filter":"grok",
      "conditions":{
        "fields":{
          "type":[
            "yarn_apps_nm_stdout1",
            "yarn_apps_nm_stderr1",
            "yarn_apps_nm_stdout2",
            "yarn_apps_nm_stderr2"
          ]
         }
       },
      "log4j_format":"%d{DATA} %-5p %c{2} (%F:%M(%L)) - %m%n",
      "multiline_pattern":"^(%{DATA:logtime})",
      "message_pattern":"(?m)^%{DATA:logtime}%{SPACE}\\[%{DATA:thread_name}\\]%{SPACE}%{LOGLEVEL:level}%{SPACE}%{JAVACLASS:logger_name}%{SPACE}-%{SPACE}%{GREEDYDATA:log_message}"
    },
    {
      "filter":"grok",
      "conditions":{
        "fields":{
          "type":[
            "yarn_apps_task1",
            "yarn_apps_task2",
            "yarn_apps_taskattempt1",
            "yarn_apps_taskattempt2"
          ]
         }
       },
      "log4j_format":"%d{TIMESTAMP_ISO8601} %-5p %c{2} (%F:%M(%L)) - %m%n",
      "multiline_pattern":"^(%{TIMESTAMP_ISO8601:logtime})",
      "message_pattern":"(?m)^%{TIMESTAMP_ISO8601:logtime}%{SPACE}\\[%{LOGLEVEL:level}\\]%{SPACE}\\[%{JAVACLASS:logger_name}\\]%{SPACE}\\|%{JAVAFILE:file}\\|:%{SPACE}%{GREEDYDATA:log_message}"
    }
  ]
}
